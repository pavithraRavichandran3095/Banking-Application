<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="match_parent"
  android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/relativeLayout1" android:layout_width="match_parent" android:layout_height="match_parent" android:background="@drawable/userback">
      <TextView android:id="@+id/textView1" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="Welcome to Wells Fargo Bank" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_alignParentTop="true" android:layout_centerHorizontal="true" android:layout_marginTop="14dp" android:textStyle="bold"></TextView>
      <TextView android:id="@+id/userbankdepuser" android:textColor="#000000" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_below="@+id/textView1" android:layout_centerHorizontal="true" android:layout_marginTop="18dp"></TextView>
      <ImageView android:id="@+id/imageView1" android:maxWidth="250dp" android:src="@drawable/depoimg1" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_below="@+id/textView1" android:layout_alignParentLeft="true"></ImageView>
      <TextView android:id="@+id/textView2" android:textColor="#000000" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="Voucher Id :" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_below="@+id/imageView1" android:layout_alignParentLeft="true"></TextView>
      <ImageView android:id="@+id/userbankdepsub" android:src="@drawable/depo" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_above="@+id/userbankdeplogout" android:layout_centerHorizontal="true" android:layout_marginBottom="14dp"></ImageView>
      <EditText android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/userdepactamount" android:layout_above="@+id/userbankdepsub" android:layout_alignParentLeft="true" android:layout_alignParentRight="true">
          <requestFocus></requestFocus>
      </EditText>
      <TextView android:layout_marginBottom="8dp" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_width="wrap_content" android:id="@+id/userbankdephome" android:layout_height="wrap_content" android:textStyle="bold" android:text="Home" android:layout_alignBaseline="@+id/userbankdeplogout" android:layout_alignBottom="@+id/userbankdeplogout" android:layout_toLeftOf="@+id/userbankdepsub"></TextView>
      <TextView android:layout_marginBottom="8dp" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_width="wrap_content" android:id="@+id/userbankdeplogout" android:layout_height="wrap_content" android:textStyle="bold" android:text="Log out" android:layout_alignParentBottom="true" android:layout_alignRight="@+id/textView1"></TextView>
   </RelativeLayout>
</LinearLayout>
