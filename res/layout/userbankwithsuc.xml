<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="match_parent"
  android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/relativeLayout1" android:layout_width="match_parent" android:layout_height="match_parent" android:background="@drawable/userback">
      <TextView android:id="@+id/textView1" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="Welcome to Wells Fargo Bank" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_alignParentTop="true" android:layout_centerHorizontal="true" android:layout_marginTop="14dp" android:textStyle="bold"></TextView>
      <TextView android:id="@+id/userbankwithlogout" android:layout_height="wrap_content" android:layout_width="wrap_content" android:text="Log out" android:textStyle="bold" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true" android:layout_marginBottom="8dp"></TextView>
      <TextView android:id="@+id/userbankdepuser" android:textColor="#000000" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_below="@+id/textView1" android:layout_centerHorizontal="true" android:layout_marginTop="18dp"></TextView>
      <Button android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/button1" android:text="Native" android:layout_above="@+id/userbankwithlogout" android:layout_alignLeft="@+id/userbankwithlogout" android:layout_marginBottom="15dp"></Button>
      <TextView android:text="Saved in mnt/sdcard/CovertFiles" android:layout_height="wrap_content" android:textColor="#B20909" android:id="@+id/textView4" android:layout_width="wrap_content" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_above="@+id/button1" android:layout_alignLeft="@+id/textView1" android:layout_marginBottom="19dp"></TextView>
      <ImageView android:maxWidth="250dp" android:src="@drawable/cheque" android:id="@+id/imageView1" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_alignTop="@+id/userbankdepuser" android:layout_alignParentRight="true" android:layout_alignParentLeft="true"></ImageView>
      <TextView android:text="E-Slip was downloaded Successfully" android:layout_height="wrap_content" android:textColor="#B20909" android:id="@+id/textView3" android:layout_width="wrap_content" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_above="@+id/textView4" android:layout_centerHorizontal="true" android:layout_marginBottom="18dp"></TextView>
      <TextView android:text="Thank You." android:layout_height="wrap_content" android:textColor="#B20909" android:id="@+id/textView2" android:layout_width="wrap_content" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_above="@+id/textView3" android:layout_centerHorizontal="true" android:layout_marginBottom="16dp"></TextView>
   </RelativeLayout>
</LinearLayout>
