<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="match_parent"
  android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/relativeLayout1" android:layout_height="match_parent" android:layout_width="match_parent" android:background="@drawable/userback">
        <TextView android:layout_alignParentTop="true" android:layout_marginTop="14dp" android:layout_width="wrap_content" android:textStyle="bold" android:layout_centerHorizontal="true" android:textAppearance="?android:attr/textAppearanceMedium" android:text="Welcome to Wells Fargo Bank" android:id="@+id/textView1" android:layout_height="wrap_content"></TextView>
        <TextView android:layout_marginBottom="8dp" android:layout_width="wrap_content" android:textStyle="bold" android:layout_centerHorizontal="true" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_alignParentBottom="true" android:text="Log out" android:id="@+id/userbankactlogout" android:layout_height="wrap_content"></TextView>
        <TextView android:layout_width="wrap_content" android:textColor="#000000" android:textAppearance="?android:attr/textAppearanceMedium" android:text="  Deposit  " android:id="@+id/textView2" android:layout_height="wrap_content" android:layout_below="@+id/userbankactdeposit" android:layout_alignLeft="@+id/userbankactdeposit" android:layout_marginTop="16dp"></TextView>
        <ImageButton android:id="@+id/userbankactwith" android:layout_width="wrap_content" android:src="@drawable/with" android:layout_height="wrap_content" android:layout_above="@+id/textView2" android:layout_alignRight="@+id/textView1"></ImageButton>
        <TextView android:layout_width="wrap_content" android:textColor="#000000" android:textAppearance="?android:attr/textAppearanceMedium" android:text="   Withdrawal  " android:id="@+id/textView3" android:layout_height="wrap_content" android:layout_alignBaseline="@+id/textView2" android:layout_alignBottom="@+id/textView2" android:layout_toRightOf="@+id/userbankactlogout"></TextView>
        <ImageButton android:id="@+id/userbankactdeposit" android:layout_width="wrap_content" android:src="@drawable/deposit" android:layout_height="wrap_content" android:layout_below="@+id/textView1" android:layout_alignLeft="@+id/textView1" android:layout_marginTop="45dp"></ImageButton>
        <ImageButton android:id="@+id/userbankacttrans" android:layout_width="wrap_content" android:src="@drawable/trans" android:layout_height="wrap_content" android:layout_below="@+id/textView2" android:layout_alignRight="@+id/userbankactdeposit" android:layout_marginTop="40dp"></ImageButton>
        <ImageButton android:id="@+id/userbankactlist" android:layout_width="wrap_content" android:src="@drawable/im" android:layout_height="wrap_content" android:layout_above="@+id/textView4" android:layout_alignLeft="@+id/userbankactwith"></ImageButton>
        <TextView android:layout_width="wrap_content" android:textColor="#000000" android:textAppearance="?android:attr/textAppearanceMedium" android:text="   E-Slip" android:id="@+id/textView5" android:layout_height="wrap_content" android:layout_alignBaseline="@+id/textView4" android:layout_alignBottom="@+id/textView4" android:layout_alignLeft="@+id/userbankactlist"></TextView>
        <TextView android:layout_width="wrap_content" android:textColor="#000000" android:textAppearance="?android:attr/textAppearanceMedium" android:text="Transfer" android:id="@+id/textView4" android:layout_height="wrap_content" android:layout_below="@+id/userbankacttrans" android:layout_alignRight="@+id/userbankacttrans" android:layout_marginTop="18dp"></TextView>
        <TextView android:layout_width="wrap_content" android:textColor="#CD0000" android:textAppearance="?android:attr/textAppearanceSmall" android:text="" android:id="@+id/userbankactdepsuc" android:layout_height="wrap_content" android:layout_below="@+id/textView2" android:layout_alignParentLeft="true"></TextView>
        <TextView android:layout_width="wrap_content" android:textColor="#CD0000" android:textAppearance="?android:attr/textAppearanceSmall" android:text="" android:id="@+id/userbankactwithdsuc" android:layout_height="wrap_content" android:layout_below="@+id/textView3" android:layout_alignLeft="@+id/textView3"></TextView>
        <TextView android:layout_width="wrap_content" android:textColor="#CD0000" android:textAppearance="?android:attr/textAppearanceSmall" android:text="" android:id="@+id/userbankactfundsuc" android:layout_height="wrap_content" android:layout_below="@+id/textView4" android:layout_alignParentLeft="true"></TextView>
        <TextView android:text=" Balance" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_height="wrap_content" android:id="@+id/textView6" android:textColor="#000000" android:layout_width="wrap_content" android:layout_below="@+id/userbankactmoney" android:layout_alignLeft="@+id/userbankactmoney"></TextView>
        <ImageButton android:id="@+id/userbankactmoney" android:src="@drawable/moneybal" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_alignTop="@+id/userbankactfundsuc" android:layout_alignLeft="@+id/userbankactlogout"></ImageButton>        
    </RelativeLayout>
</LinearLayout> 
